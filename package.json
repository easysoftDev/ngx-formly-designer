{
  "name": "ngx-formly-designer-app",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "debug": "concurrently \"ng serve -c development\"  \"ng build ngx-formly-designer --watch\"",
    "start": "npm run build_lib && ng serve --prod",
    "build": "ng build ngx-formly-designer && ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "copy": "node tools/copy.js README.md dist/ngx-formly-designer/README.md && node tools/copy.js LICENSE dist/ngx-formly-designer/LICENSE",
    "build_lib": "ng build ngx-formly-designer --prod",
    "npm_pack": "npm run copy && cd dist/ngx-formly-designer && npm pack",
    "package": "npm run build_lib && npm run npm_pack"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@ngx-formly/bootstrap": "^5.10.5",
    "@ngx-formly/core": "^5.10.5",
    "bootstrap": "^4.1.3",
    "core-js": "^2.6.4",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "lodash-es": "^4.17.11",
    "ng2-dragula": "^2.1.1",
    "ngx-formly-tabs": "^5.1.0",
    "popper.js": "^1.14.7",
    "rxjs": "^6.6.3",
    "tsickle": "^0.39.1",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.29",
    "@angular-devkit/build-ng-packagr": "~0.803.29",
    "@angular/cli": "~8.3.29",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/jquery": "~3.3.29",
    "@types/lodash-es": "~4.17.1",
    "@types/node": "~10.5.6",
    "codelyzer": "^5.0.1",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.5",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^1.2.0",
    "ng-packagr": "^5.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.1",
    "tslib": "^1.14.1",
    "tslint": "~5.11.0",
    "typescript": "3.5.3"
  }
}
